<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/constraintLayout4"
            motion:layout_constraintEnd_toEndOf="@+id/constraintLayout3"
            android:layout_width="220dp"
            android:layout_height="65dp"
            android:layout_marginBottom="-20dp"
            motion:layout_constraintBottom_toTopOf="@+id/constraintLayout3"
            motion:layout_constraintStart_toStartOf="@+id/constraintLayout3"
            android:translationX="-1000dp" />
        <Constraint
            android:id="@+id/constraintLayout3"
            motion:layout_constraintEnd_toEndOf="@+id/bg1"
            android:layout_width="250dp"
            android:layout_height="250dp"
            motion:layout_constraintBottom_toBottomOf="@+id/bg1"
            android:layout_marginStart="80dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="108dp"
            android:translationX="-1000dp" />
        <Constraint
            android:id="@+id/imageView2"
            motion:layout_constraintEnd_toEndOf="@+id/constraintLayout4"
            android:layout_width="95dp"
            android:layout_height="85dp"
            android:layout_marginBottom="-20dp"
            motion:layout_constraintBottom_toTopOf="@+id/constraintLayout4"
            motion:layout_constraintStart_toStartOf="@+id/constraintLayout4"
            android:translationX="-1000dp" />
        <Constraint
            android:id="@+id/constraintLayout5"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="70dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="48dp"
            motion:layout_constraintTop_toTopOf="@+id/imageView"
            android:translationX="1000dp" />
        <Constraint
            android:id="@+id/plusBut"
            android:elevation="0dp"
            motion:layout_constraintEnd_toEndOf="@+id/constraintLayout5"
            android:layout_width="70dp"
            android:layout_height="70dp"
            motion:layout_constraintTop_toBottomOf="@+id/constraintLayout5"
            motion:layout_constraintStart_toStartOf="@+id/constraintLayout5"
            android:layout_marginTop="-10dp"
            android:translationX="1000dp" />
        <Constraint
            android:id="@+id/textView3"
            android:elevation="10dp"
            motion:layout_constraintEnd_toEndOf="@+id/plusBut"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/plusBut"
            motion:layout_constraintTop_toTopOf="@+id/plusBut"
            motion:layout_constraintStart_toStartOf="@+id/plusBut"
            android:translationX="1000dp" />
        <Constraint
            android:id="@+id/minusBut"
            android:elevation="0dp"
            motion:layout_constraintEnd_toEndOf="@+id/constraintLayout5"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_marginBottom="-10dp"
            motion:layout_constraintBottom_toTopOf="@+id/constraintLayout5"
            motion:layout_constraintStart_toStartOf="@+id/constraintLayout5"
            android:translationX="1000dp" />
        <Constraint
            android:id="@+id/textView2"
            android:elevation="10dp"
            motion:layout_constraintEnd_toEndOf="@+id/minusBut"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@+id/minusBut"
            motion:layout_constraintTop_toTopOf="@+id/minusBut"
            motion:layout_constraintStart_toStartOf="@+id/minusBut"
            android:translationX="1000dp" />
        <Constraint
            android:id="@+id/spinBut"
            motion:layout_constraintEnd_toEndOf="@+id/plusBut"
            android:layout_width="wrap_content"
            android:layout_height="70dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp"
            motion:layout_constraintTop_toBottomOf="@+id/plusBut"
            motion:layout_constraintStart_toStartOf="@+id/plusBut"
            android:visibility="invisible" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>